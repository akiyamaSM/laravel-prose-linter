{
    "name": "beyondcode/laravel-prose-linter",
    "description": "Aweeeeesomeeeee",
    "keywords": [
        "beyondcode",
        "laravel-prose-linter"
    ],
    "homepage": "https://github.com/beyondcode/laravel-prose-linter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Diana Scharf",
            "email": "diana@beyondco.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "symfony/finder": "^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Beyondcode\\LaravelProseLinter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beyondcode\\LaravelProseLinter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beyondcode\\LaravelProseLinter\\LaravelProseLinterServiceProvider"
            ]
        }
    }
}
